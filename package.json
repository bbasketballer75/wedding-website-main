{
  "name": "wedding-website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy:prod": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:check": "pwsh -ExecutionPolicy Bypass -File scripts/deploy-vercel.ps1 -CheckOnly",
    "dev": "cross-env PORT=3001 next dev",
    "dev:start": "pwsh -ExecutionPolicy Bypass -File scripts/dev-startup.ps1",
    "dev:clean": "pwsh -ExecutionPolicy Bypass -File scripts/dev-startup.ps1 -Clean",
    "dev:force": "pwsh -ExecutionPolicy Bypass -File scripts/dev-startup.ps1 -Force",
    "dev:validate": "node scripts/validate-dev-env.mjs",
    "dev:full": "npm run dev:emulator & npm run dev:backend & npm run mcp:sequential & npm run dev",
    "dev:emulator": "firebase emulators:start --only firestore",
    "dev:backend": "cd backend && cross-env NODE_ENV=development FIRESTORE_EMULATOR_HOST=localhost:8082 PORT=3002 npm start",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:vercel": "next build && echo 'Build completed for Vercel'",
    "convert:webp": "node scripts/convert-to-webp.js",
    "audit:a11y": "node scripts/accessibility-audit.mjs",
    "validate:optimizations": "node scripts/validate-optimizations.mjs",
    "mcp:sequential": "node scripts/sequential-thinking-manager.mjs",
    "mcp:status": "node scripts/check-mcp-servers.mjs",
    "start": "next start",
    "start:prod": "npm run build && next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix:all": "npm run lint:fix && npm run format",
    "fix:pre-commit": "node scripts/pre-commit-fix.mjs",
    "integrate:features": "node scripts/integration-manager.mjs",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "vitest run",
    "test:backend": "cd backend && npm test",
    "test:watch": "vitest",
    "test:unit": "vitest run --project unit",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:headless": "cypress run --headless",
    "test:e2e": "cypress run",
    "test:all": "npm run test && npm run test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "emulator": "firebase emulators:start --only firestore",
    "optimize": "node scripts/optimize-project.mjs",
    "clean:cache": "rm -rf .next node_modules/.cache",
    "analyze:deps": "npx depcheck"
  },
  "dependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@modelcontextprotocol/server-sequential-thinking": "^2025.7.1",
    "@next/bundle-analyzer": "^15.4.5",
    "@sentry/nextjs": "^10.0.0",
    "@storybook/addon-a11y": "^9.0.18",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-onboarding": "^9.0.18",
    "@storybook/addon-vitest": "^9.0.18",
    "@storybook/nextjs-vite": "^9.0.18",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.11.0",
    "babel-plugin-istanbul": "^7.0.0",
    "chalk": "^5.5.0",
    "cypress-axe": "^1.6.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.4",
    "eslint-plugin-storybook": "^9.0.18",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.535.0",
    "next": "^14.2.15",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "storybook": "^9.0.18",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/eslintrc": "^3.3.1",
    "@mozilla/readability": "^0.6.0",
    "@playwright/test": "^1.54.2",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-jest": "^30.0.5",
    "canvas": "^3.1.2",
    "cross-env": "^10.0.0",
    "cypress": "^14.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "storybook": "^9.0.18",
    "vercel": "^44.7.2",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "path-to-regexp": "^8.0.0",
    "undici": "^6.0.0",
    "esbuild": "^0.25.0",
    "glob": "^10.4.0"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=8.0.0"
  }
}
